<Page x:ConnectionId='1'
    x:Class="Results.AccountManager.Views.CompletedView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Results.AccountManager.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <StackPanel HorizontalAlignment="Center"
                  VerticalAlignment="Center">
            <TextBlock x:ConnectionId='2' x:Name="Name"
                       FontSize="50"
                       Foreground="ForestGreen"
                                                   
                       />
            <TextBlock x:ConnectionId='3' x:Name="Email"                       
                       HorizontalAlignment="Center"
                                                    
                       Margin="0 20"
                       />
            <Button x:ConnectionId='4' x:Name="RegiterExam" 
                    Content="Add Exams"
                    HorizontalAlignment="Center"
                                             
                    />
            <Button x:ConnectionId='5' x:Name="BackBtn" 
                    Content="Back"
                    HorizontalAlignment="Center"
                                         
                    />
        </StackPanel>
    </Grid>
</Page>

